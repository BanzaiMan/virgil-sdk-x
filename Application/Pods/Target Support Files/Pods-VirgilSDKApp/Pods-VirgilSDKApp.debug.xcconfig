FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/VirgilFoundation/Frameworks/ios"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(FRAMEWORK_SEARCH_PATHS)/VirgilCrypto.framework/Headers $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/VirgilFoundation"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/VirgilFoundation"
OTHER_LDFLAGS = $(inherited) -ObjC -l"VirgilFoundation" -l"stdc++" -framework "VirgilCrypto"
PODS_ROOT = ${SRCROOT}/Pods
